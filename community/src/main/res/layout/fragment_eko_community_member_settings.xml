<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".members.EkoCommunityMemberSettingsFragment">

    <com.ekoapp.ekosdk.uikit.components.EkoToolBar
        android:id="@+id/membersToolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/BaseToolBarStyle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <com.ekoapp.ekosdk.uikit.components.EkoTabLayout
        android:id="@+id/membersTabLayout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/membersToolbar"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>